#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    if(n==2 || n==3){
        cout << "NO SOLUTION" << endl;
        return 0;
    }
    int num = 2;
    while (num <= n){
        cout << num << " ";
        num +=2;
    }
    num = 1;
    while (num <= n){
        cout << num << " ";
        num += 2;
    }
    return 0;
}
